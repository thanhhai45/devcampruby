<h1>Homepage</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<table>
  <thead>
    <th>ID</th>
    <th>Title</th>
    <th>Bodies</th>
    <th>Created_At</th>
    <th>Updated_At</th>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= post.created_at %></td>
        <td><%= post.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= @posts.inspect %>

<table>
  <thead>
  <th>ID</th>
  <th>Title</th>
  <th>Percent Utilized</th>
  <th>Created_At</th>
  <th>Updated_At</th>
  </thead>
  <tbody>
  <% @skills.each do |skill| %>
      <tr>
        <td><%= skill.id %></td>
        <td><%= skill.title %></td>
        <td><%= skill.percent_utilized %></td>
        <td><%= skill.created_at %></td>
        <td><%= skill.updated_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>